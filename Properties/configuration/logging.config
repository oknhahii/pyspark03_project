[loggers]
keys = root, create_spark, validate, ingest, data_processing, daat_transformation, persist, extraction, load_postgresql, email_handling
[handlers]
keys = consoleHandler, fileHandler

[formatters]
keys = sampleFormatter

[logger_root]
level = DEBUG
handlers = consoleHandler, fileHandler

[logger_create_spark]
level = INFO
handlers = fileHandler
qualname = Create_spark
propagate = 0

[logger_validate]
level = INFO
handlers = fileHandler
qualname = Validate
propagate = 0

[logger_ingest]
level = WARN
handlers = fileHandler
qualname = Ingest
propagate = 0

[logger_data_processing]
level = WARN
handlers = fileHandler
qualname = Data_processing
propagate = 0

[logger_daat_transformation]
level = WARN
handlers = fileHandler
qualname = Data_transformation
propagate = 0

[logger_persist]
level = WARN
handlers = fileHandler
qualname = Persist
propagate = 0

[logger_extraction]
level = WARN
handlers = fileHandler
qualname = Extraction
propagate = 0

[logger_load_postgresql]
level = WARN
handlers = fileHandler
qualname = Load_postgresql
propagate = 0


[logger_email_handling]
level = WARN
handlers = fileHandler
qualname = Email_handling
propagate = 0


[handler_consoleHandler]
class = StreamHandler
level = DEBUG
formatter = sampleFormatter
args = (sys.stdout,)

[handler_fileHandler]
class = logging.FileHandler
level = DEBUG
formatter = sampleFormatter
args = ('application.log',)

[formatter_sampleFormatter]
format = %(asctime)s - %(name)s -%(levelname)s -%(message)s